<h2>New physician search</h2>
<%= form_tag(method: "get") do %>
  <p>
  <%= label_tag(:address, "Search by patient address:") %>
  <%= text_field_tag(:address) %>
  </p>
  OR
  <p>
  <%= label_tag(:zipcode, "zipcode:") %>
  <%= text_field_tag(:zipcode) %>
  </p>
  <%= submit_tag("Search") %>
<% end %>